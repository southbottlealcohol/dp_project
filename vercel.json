{
    "builds": [
      {
        "src": "deepfake/django_app/project_settings/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        },
        "hooks": {
          "preBuild": {
            "commands": [
              "mkdir -p /vercel/path0",
              "pip install --upgrade pip",
              "pip install -r /vercel/path0/requirements.txt"
            ]
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "deepfake/django_app/project_settings/wsgi.py"
      }
    ],
    "env": {
      "PIP_TARGET": "/vercel/path0",
      "PIP_CACHE_DIR": "/vercel/path0/pip-cache"
    }
  }
  